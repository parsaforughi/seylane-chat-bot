{
  "name": "seylane-chat-bot",
  "version": "1.0.0",
  "description": "Seylane Chat Bot - AI-powered Instagram chatbot with GPT and WooCommerce integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch server/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd client && npm install --legacy-peer-deps && npm run build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "setup": "npm install && cd client && npm install --legacy-peer-deps && cd .. && npm run db:push"
  },
  "keywords": ["instagram", "chatbot", "gpt", "woocommerce", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "axios": "^1.7.9",
    "concurrently": "^8.2.2",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "nanoid": "^5.0.9",
    "openai": "^4.77.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.19.0",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/ws": "^8.5.13",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}

